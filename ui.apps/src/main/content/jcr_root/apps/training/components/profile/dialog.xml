<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Title Component"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Update header value">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Name"
                        fieldLabel="Name"
                        name="./name"
                        xtype="textfield"/>
                    <country
                        jcr:primaryType="cq:Widget"
                        fieldDescription="country"
                        fieldLabel="country"
                        name="./country"
                        xtype="textfield"/>
                    <practice-area
                        jcr:primaryType="cq:Widget"
                        fieldDescription="practice area"
                        fieldLabel="Practice area"
                        name="./practiceArea"
                        xtype="textfield"/>
                    <sector
                        jcr:primaryType="cq:Widget"
                        fieldDescription="sector"
                        fieldLabel="Sector"
                        name="./sector"
                        xtype="textfield"/>
                    <telephone
                        jcr:primaryType="cq:Widget"
                        fieldDescription="telephone"
                        fieldLabel="Telephone"
                        name="./telephone"
                        xtype="textfield"/>
                    <image
                        jcr:primaryType="cq:Widget"
                        fieldDescription="image"
                        fieldLabel="Image"
                        name="./image"
                        xtype="fileuploadfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
